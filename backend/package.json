  "scripts": {
    "start:dev": "nodemon --ext js,json,ts --watch src/ --exec \"npm run build && npm run start\" & mongod --dbpath /tmp --port 27021 > /tmp/mongo_db_test.log && fg",
    "build": "rm -rf build && babel src --out-dir build --extensions \".ts\" && tsc",
    "start": "babel-node -x .ts -- src/index.ts",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "npm run prettier -- --write",
    "test": "sudo service mongod start && env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' && mocha --recursive --exit"
  },
